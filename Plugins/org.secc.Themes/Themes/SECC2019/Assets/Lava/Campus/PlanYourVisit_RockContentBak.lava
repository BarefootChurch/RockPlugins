<section id="Location">
    <div class="container-fluid">
        <div class="row g-padding-y-150--md g-padding-y-50--xs">

            {[ wow animate:'fadeInLeft' duration:'.8s' ]}
            <div class="col-sm-12 col-lg-7 g-padding-l-0--xs g-padding-r-100--xs g-margin-t-50--xs">
                <div class="g-position--relative g-padding-l-80--md g-padding-y-30--xs g-hor-divider__solid__b--black clearfix" style="border-width: 5px;">
                    <h1 class="g-font-size-200--xs g-font-weight--900 g-width-150--xs pull-left text-center">1</h1>
                    <h1 class="g-font-family--secondary
                        g-width-70-percent--xs
                        g-font-size-60--xs
                        g-padding-l-50--xs
                        g-margin-l-150--xs
                        g-font-size-75--md
                        g-width-80-percent--md
                        g-font-size-90--lg
                        pull-left
                        text-left
                        g-ver-center--xs">
                        Choose a Location & Service
                    </h1>
                </div>
            </div>
            {[ endwow ]}

            {[ wow animate:'fadeIn' duration:'.8s' delay:'.8s' ]}
            <div class="col-sm-12 col-lg-5 g-margin-t-50--xs g-margin-t-100--lg g-padding-x-20--xs g-padding-x-150--md">
                <h3 class="g-line-height g-margin-b-0--xs">
                    <strong>Southeast Christian Church is one church with multiple locations.</strong><br>
                    Our mission is connecting people to Jesus and one another by loving where we are. We believe bringing the church to more communities is a great way to reach more people with Jesus’ love.
                </h3>
            </div>
            {[ endwow ]}
        </div>
    </div>
</section>

